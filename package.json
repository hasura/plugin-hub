    "express": "^4.18.2",
    "flat": "^6.0.1",
    "graphql": "^16.9.0",
    "into-stream": "^8.0.1",
    "node-cron": "^3.0.3",
    "winston": "^3.15.0"
  },
  "devDependencies": {
